{
  "name": "vtpass",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "push-ios": "appcenter codepush release-react -a oluwaturheeb-gmail.com/vtpass-Android -d Staging",
    "push-android": "appcenter codepush release-react -a oluwaturheeb-gmail.com/vtpass-Android -d Staging",
    "create-deployment": "appcenter apps create -d vtpass-Android -o Android -p React-Native",
    "generate-staging-key": "appcenter codepush deployment add -a oluwaturheeb-gmail.com/vtpass-Android Staging",
    "generate-production-key": "appcenter codepush deployment add -a oluwaturheeb-gmail.com/vtpass-Android Production",
    "list-keys": "appcenter codepush deployment list -a oluwaturheeb-gmail.com/vtpass-Android -k",
    "appcenter-login": "appcenter login",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "bundle-android-assets": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "bundle-ios": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "package-release": "react-native run-android --variant release",
    "install-debug": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "install-release": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "clean": "react-native clean"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "axios": "^1.2.2",
    "base-64": "^1.0.0",
    "flutterwave-react-native": "^1.0.2",
    "lodash.filter": "^4.6.0",
    "react": "18.2.0",
    "react-native": "0.71.7",
    "react-native-biometrics": "^3.0.1",
    "react-native-code-push": "^7.1.0",
    "react-native-gesture-bottom-sheet": "^1.1.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-paper": "^5.1.3",
    "react-native-reanimated": "^3.0.2",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-select-contact": "^1.6.3",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^11.26.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/base-64": "^1.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash.filter": "^4.6.7",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
